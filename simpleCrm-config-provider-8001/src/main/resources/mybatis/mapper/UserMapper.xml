<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="simplecrm.dao.IUserDao">
    <select id="loginByUsernameAndPassword" resultType="com.aoao.simpleCrm.entities.Employee">
        select * from t_dams_employee where emp_loginname = #{username} and emp_password = #{password}
    </select>
</mapper>